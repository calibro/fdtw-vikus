{
    "project": {
      "name": "FDTW",
      "quality": 1
   },
    "loader": {
      "info": "http://127.0.0.1:8080/data/info.md",
      "timeline": "http://127.0.0.1:8080/data/timeline.csv",
      "items": "http://127.0.0.1:8080/data/data.csv",
      "textures": {
        "medium": {
          "size": 256,
          "url": "http://127.0.0.1:8080/data/sprites/spritesheet.json"
        },
        "detail": {
          "size": 1024,
          "url": "http://127.0.0.1:8080/data/1024/"
        },
        "big": {
          "size": 4096,
          "url": "http://127.0.0.1:8080/data/4096/"
        }
      }
    },
    "style": {
      "fontColor": "#2d3b43",
      "fontColorActive": "#eae2cb",
      "fontBackground": "#295b71",
      "textShadow": "1px 1px 0px #f8f4e9",
      "canvasBackground": "#eae2cb",
      "timelineBackground": "#fff9eb",
      "timelineFontColor": "#000",
      "backgroundHeader": "#035151",
      "fontColorHeader": "#f8774f",
      "detailBackground": "#EEE",
      "infoBackground": "#33444c",
      "infoFontColor": "#fff",
      "searchbarBackground": "#295b71"
    },
    "projection": {
      "columns": 6
    },
    "detail": {
      "structure": [      
        {
          "name": "Description",
          "source": "_description",
          "display": "column",
          "type": "text"
        },
        {
          "name": "Century",
          "source": "_century",
          "display": "column",
          "type": "text"
        },
        {
          "name": "Collection",
          "source": "_collection",
          "display": "column",
          "type": "text"
        },
        {
          "name": "Dimensions",
          "source": "column._width + 'mm * ' + column._height + 'mm'",
          "display": "column",
          "type": "function"
        },
        {
          "name": "Keywords",
          "source": "_keywords",
          "display": "column",
          "type": "array"
        },
        {
          "name": "Id",
          "source": "id",
          "display": "column",
          "type": "text"
        }
      ]
    }
  }